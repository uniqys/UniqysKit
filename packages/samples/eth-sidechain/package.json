    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "vue": "^2.5.16",
    "web3": "1.0.0-beta.37"
  },
  "devDependencies": {
    "@types/debug": "0.0.31",
    "@types/koa": "^2.0.46",
